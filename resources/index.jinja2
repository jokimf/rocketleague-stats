        font-weight: 300;
        line-height: 1em;
        text-align: center;
        color: #1d7aa4;
    }

    h2 {
        text-align: left;
        display: block;
        font-size: 2em;
        margin-block-start: 0.67em;
        margin-block-end: 0.67em;
    }

    h3 {
        text-align: center;
        display: block;
        font-size: 2em;
        margin-block-start: 0.67em;
        margin-block-end: 0.67em;
    }

    h4 {
        text-align: center;
        font-size: 2em;
        font-weight: 600;
        margin-top: 0;
    }

    .profile-pics-flexbox {
        display: flex;
        justify-content: center;
        gap: 150px
    }

    .profile-pics {
        margin: 10px;
        border-radius: 100px;
        border: 1px solid;
        height: 100px;
    }

    .icon {
        width: 22px;
        height: 22px;
        max-height: 22px;
        max-width: 22px;
    }

    figure {
        display: flex;
        flex-flow: column-reverse;
        padding: 5px;
        max-width: 220px;
        margin: auto;
    }

    figcaption {
        font-weight: bolder;
        font-size: 22px;
        background-color: #dedede;
        text-align: center;
    }

    .sub-headline {
        font-size: 1em;
        font-weight: 300;
        line-height: 1em;
        color: #1d7aa4;
    }

    .infobox {
        margin: 5px;
    }

    .total-season-session {
        display: flex;
        justify-content: center;
    }

    .random-facts {
        text-align: center;
    }

    /** Reset some basic elements */
    blockquote, pre, hr, dl, dd, ol, ul, figure {
        margin: 0;
        padding: 0;
    }

    table {
        border-collapse: collapse;
        font-style: normal;
        margin-right: auto;
        margin-left: auto;
    }

    caption {
        font-size: 2em;
        line-height: 1.8em;
    }

    th {
        color: #000000;
        font-weight: bold;
    }

    td, th {
        border: 1px solid #999;
        padding: 0.5rem;
        text-align: center;
    }

    .infobox td {
        font-size: smaller;
    }

    table tr:nth-child(even) {
        background-color: #f7f7f7;
    }

    table th {
        background-color: #f0f0f0;
        border: 1px solid #dedede;
        border-bottom-color: #c9c9c9;
    }

    table td {
        border: 1px solid #cccccc;
    }

    li {
        text-align: left;
        color: #353535;
        font-size: 16px;
        font-weight: 400;
        line-height: 1.5;
    }

    ul {
        padding-left: 32px;
    }

    .graph-shell-small {
        height: 200px;
        background-color: #eeeeee;
    }

    .graph-shell-big {
        height: 700px;
    }

    hr {
        border: 0;
        padding: 0;
        height: 1px;
        background-image: linear-gradient(to right, rgba(0, 0, 0, 0), rgba(40, 40, 40, 0.3), rgba(0, 0, 0, 0));
        margin-block-start: 20px;
        margin-block-end: 20px;
    }

    button {
        transition: background .25s, border-color .25s;
        background: rgba(40, 44, 52, .05);
        border: 1px solid transparent;
        border-radius: 6px;
        color: #000000;
        text-decoration: none !important;
        display: inline-block;
        font-size: .8rem;
        padding: 8px 16px;
        margin: 0 8px 8px 0;
        cursor: pointer;
        -webkit-user-select: none;
        -moz-user-select: none;
        user-select: none;
    }

    .records {
        display: flex;
        justify-content: center;
        gap: 25px;
    }

    .winrate-flexbox {
        display: flex;
        justify-content: center;
        gap: 100px;
        margin-bottom: 30px;
    }

    .winrate-desc-flexbox {
        gap: 160px;
        display: flex;
        justify-content: center;
    }

    .winrate {
        font-size: 2em;
        font-weight: bolder;
    }

    .winrate-desc {
        font-size: smaller;
    }
</style>
