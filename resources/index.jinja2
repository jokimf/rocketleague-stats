    <div class="profile-pics-flexbox">
        <figure>
            <figcaption><img alt={{ ranks[0] }} class="icon" src="https://jok.im/static/img/{{ ranks[0] }}.png"> Knus
            </figcaption>
            <img class="profile-pics"
                 src="https://avatars.akamai.steamstatic.com/c63017936fd4aa600bf832af661314aeae2991dd_full.jpg"
                 alt="Knus">
        </figure>
        <figure>
            <figcaption><img alt={{ ranks[1] }} class="icon" src="https://jok.im/static/img/{{ ranks[1] }}.png"> Puad
